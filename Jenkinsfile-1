pipeline {
    agent any
    environment {
        URL='google.com'
    }
    stages {
        stage('One'){
            steps {
                sh 'echo URL=${URL}'
            }
        }
        stage('two') {
            steps {
                sh 'echo welcome to stage 2'
            }
        }
        stage('three') {
            steps {
                sh 'echo welcome to stage three'
            }
        }
    } //stages

} //pipeline